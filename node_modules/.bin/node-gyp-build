#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/user-management/node_modules/.pnpm/node-gyp-build@4.8.1/node_modules/node-gyp-build/node_modules:/home/theia/user-management/node_modules/.pnpm/node-gyp-build@4.8.1/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/node-gyp-build/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/user-management/node_modules/.pnpm/node-gyp-build@4.8.1/node_modules/node-gyp-build/node_modules:/home/theia/user-management/node_modules/.pnpm/node-gyp-build@4.8.1/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/node-gyp-build/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/bin.js" "$@"
else
  exec node  "$basedir/../node-gyp-build/bin.js" "$@"
fi
