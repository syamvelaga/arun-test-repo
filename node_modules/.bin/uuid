#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/theia/user-management/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/bin/node_modules:/home/theia/user-management/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/node_modules:/home/theia/user-management/node_modules/.pnpm/uuid@3.3.2/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/uuid/bin/node_modules:/home/theia/user-management/node_modules/uuid/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/theia/user-management/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/bin/node_modules:/home/theia/user-management/node_modules/.pnpm/uuid@3.3.2/node_modules/uuid/node_modules:/home/theia/user-management/node_modules/.pnpm/uuid@3.3.2/node_modules:/home/theia/user-management/node_modules/.pnpm/node_modules:/home/theia/user-management/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/user-management/node_modules/uuid/bin/node_modules:/home/theia/user-management/node_modules/uuid/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/bin/uuid" "$@"
else
  exec node  "$basedir/../uuid/bin/uuid" "$@"
fi
